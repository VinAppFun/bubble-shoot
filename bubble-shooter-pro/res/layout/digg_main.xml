<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@drawable/appdig_bg">
	<ImageView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_gravity="right" android:layout_marginRight="6dip"
		android:layout_marginTop="6dip" android:id="@+id/close" android:src="@drawable/dig_close_selector"/>
	<ImageView android:layout_width="wrap_content"
		android:layout_gravity="bottom|left" android:id="@+id/download" android:layout_height="wrap_content"
		android:layout_marginBottom="6dip" android:layout_marginLeft="6dip" android:src="@drawable/dig_download_selector"/>
</FrameLayout>